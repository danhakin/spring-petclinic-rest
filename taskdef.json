{
    "family": "spring-petclinic-rest",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "spring-petclinic-rest",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 9966,
                    "hostPort": 9966
                }
            ]
        }
    ]
}